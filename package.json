    "prebuild:ios:run": "expo run:ios",
    "eas:build:android": "eas build --platform android",
    "eas:build:ios": "eas build --platform all",
    "test": "jest --watchAll",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.4",
