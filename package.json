  "version": "0.0.1",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prebuild:android": "npx expo prebuild --clean --platform android",
    "prebuild:ios": "npx expo prebuild --clean --platform ios",
